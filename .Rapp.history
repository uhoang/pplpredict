###########
########## CLEAN CATEGORICAL VARIABLES#
###########
#
# varName <- M$incomeRaw#
# category <- "incomeRaw"#
#
# varName <- M$voteChoicePartyLeaningRaw#
# category <- "voteChoicePartyLeaningRaw"#
# election <- "NewZealand2014"#
