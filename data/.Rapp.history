makeNumeric <- function(X, varNames){#
  if(any(!varNames %in% names(X))) stop("Not all variables in dataset.")#
  for(i in 1:length(varNames)){#
    colNum <- which(names(X) == varNames[i])#
    X[, colNum] <- as.numeric(X[, colNum])#
  }#
  return(X)#
}#
